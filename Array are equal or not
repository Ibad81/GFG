class Solution{
    public static boolean check(long A[],long B[],int N)
    {
        //Your code here
        Arrays.sort(A);
        Arrays.sort(B);
        int c=0;
        for(int i=0;i<N;i++){
            if(A[i]==B[i]){
            c++;
        }
        }
        if(c==N){
            return true;
        }
        else{
            return false;
        }
    }
}
